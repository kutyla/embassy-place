%section.padded.white
  %h1 Log In

  = form_for @user_session, { url: login_path, html: { id: 'new_user_session' } } do |form|
    .field
      = form.label :email
      = form.text_field :email
    .field
      = form.label :password
      = form.password_field :password

    = submit_tag "Sign In"
